#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <map>
using namespace std;


int main() {
    int n,num;
    string key,name;
    cin>>n;
    map<string,int>mp;
    while(n--){
        cin>>name>>num;
        mp.insert({name,num}); 
    }
    while(cin>>key){
        if(mp.find(key) != mp.end()){
            cout<<key<<"="<<mp.find(key)->second<<endl;

        }
        else{
            cout<<"Not found"<<endl;
        }
    }
    
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
